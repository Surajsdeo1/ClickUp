<% include ./partials/header.ejs %>
<div class="w-full min-h-screen bg-gradient-to-r from-cyan-500 to-blue-500 py-20 px-10 ">
<div class="cards flex flex-wrap gap-10 ">
    <% user.posts.forEach(function(elem)
    {%>
        <div class="w-32">
            <div class="card w-full h-40 bg-sky-800 rounded-lg overflow-hidden">
          <img class="object-cover" src="/images/uploads/<%=elem.image %>" alt="" srcset="">
            </div>
    <div><h5 class="mt-2  text-3xl"><%= elem.title%></h5>
          <p class="w-full h-3"><%=elem.description%></p>

    
    </div>

    </div>
    

   <% })%>
    
    </div>
    
</div>
<% include ./partials/footer.ejs %>